<?xml version="1.0"?>
<opencv_storage>
  <Cameras>
    <_>
      <ID>0</ID>
      <!--Physical ofset of the camera from the center of the robot-->
      <Offset>
        0 0 0
      </Offset>
      <!--Physical angle offset of the camera from forward-->
      <AngleOffset>
        0 0 0
      </AngleOffset>
      <!--Intresic Matrix for pose estimation (Dont edit manuly. Thr program will calculate these values)-->
      <CameraMatrix type_id="opencv-matrix">
        <rows>3</rows>
        <cols>3</cols>
        <dt>d</dt>
        <data>
          5.5255216863386806e+02 0. 3.1519772069009963e+02 
          0. 5.5196412081742744e+02 2.2797714670068797e+02 
          0. 0. 1.
        </data>
      </CameraMatrix>
      <!--Coefficents to compensate for lense distortion (Dont edit manuly. Thr program will calculate these values)-->
      <Distortion type_id="opencv-matrix">
        <rows>1</rows>
        <cols>5</cols>
        <dt>d</dt>
        <data>
          3.1258211315920514e-01 
          -1.7505129658578438e+00
          -5.1279987173060886e-03 
          1.8969764887319756e-03
          3.4821984237812211e+00
        </data>
      </Distortion>

      <!--Min and Max HSV thresholding values-->
      <Thresholds>
        <LowH>20</LowH>
        <HighH>51</HighH>
        <LowS>95</LowS>
        <HighS>248</HighS>
        <LowV>89</LowV>
        <HighV>255</HighV>
      </Thresholds>

      <!--Targets to detect by this specific camera-->
      <Targets>
      </Targets>

      <CalibrationConfig>
        <!--Number of inner corners in rach row-->
        <BoardWidth>9</BoardWidth>
        <!--Number of inner corners in rach colum-->
        <BoardHeight>6</BoardHeight>
        <!--Side length of chessboard squares-->
        <SquareSize>1</SquareSize>
        <!--Number of images that will be captured for calibration-->
        <NumberOfImages>50</NumberOfImages>
        <!--Will capture images automaticly when chessboard in is view if value is not 0-->
        <Automatic>1</Automatic>
        <!--Delay in ms between captures if Automatic is not 0-->
        <Delay>2500</Delay>
        <!--Uses photos instead of video capture is not 0-->
        <UsePhotos>0</UsePhotos>
        <!--List of the calibration photos used if UseUhotos is not 0-->
        <Images>
            images/calibrateImages/0000.jpg
            images/calibrateImages/0001.jpg
            images/calibrateImages/0002.jpg
            images/calibrateImages/0003.jpg
            images/calibrateImages/0004.jpg
            images/calibrateImages/0005.jpg
            images/calibrateImages/0006.jpg
            images/calibrateImages/0007.jpg
            images/calibrateImages/0008.jpg
            images/calibrateImages/0001.jpg
            images/calibrateImages/0010.jpg
            images/calibrateImages/0011.jpg
            images/calibrateImages/0012.jpg
            images/calibrateImages/0013.jpg
            images/calibrateImages/0014.jpg
            images/calibrateImages/0015.jpg
        </Images>
      </CalibrationConfig>
      <!--Configuation data for thresholding-->
      <ThresholdingConfig>
        <!--if not 0 the program will use the photos below to calibrate the camera-->
        <UsePhotos>0</UsePhotos>
        <!--Images to use id UsePhotos is not 0-->
        <Images>
          images/thresholdImages/0000.jpg
          images/thresholdImages/0001.jpg
          images/thresholdImages/0002.jpg
          images/thresholdImages/0003.jpg
          images/thresholdImages/0004.jpg
          images/thresholdImages/0005.jpg
          images/thresholdImages/0006.jpg
          images/thresholdImages/0007.jpg
          images/thresholdImages/0008.jpg
          images/thresholdImages/0009.jpg
          images/thresholdImages/0010.jpg
          images/thresholdImages/0011.jpg
          images/thresholdImages/0012.jpg
          images/thresholdImages/0013.jpg
          images/thresholdImages/0014.jpg
          images/thresholdImages/0015.jpg
        </Images>
      </ThresholdingConfig>
    </_>
  </Cameras>

  <!--Tagrgets to be detected by all cameras-->
  <GlobalTargets>
    <_>
      <!--name to identify target-->
      <Name>global_test</Name>
      <!--Shape of the target-->
      <Shape>
        0 0 
        5 0 
        5 3 
        4 3 
        4 1 
        1 1 
        1 3 
        0 3
      </Shape>
    </_>
  </GlobalTargets>
</opencv_storage>